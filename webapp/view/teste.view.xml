<mvc:View
	controllerName="com.treinamentoascanteiro.zalunosascanteiro.controller.View2"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
>
	<m:Shell id="shell">
		<m:App id="app">
			<m:pages>
				<m:Page
					id="page"
					title="{i18n>Alunos}"
					navButtonPress="onNavBack"
					showNavButton="true"
				>
					<m:content>
						<ObjectPageLayout
							id="ObjectPageLayout"
							upperCaseAnchorBar="false"
						>

						
							<sections>
								<ObjectPageSection
									titleUppercase="false"
									id="goalsSection"
									title="2014 Goals Plan"
								>
									<subSections>
										<ObjectPageSubSection
											id="goalsSectionSS1"
											titleUppercase="false"
										>
											<blocks>
												<goals:GoalsBlock
													id="goalsblock"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
											</blocks>
										</ObjectPageSubSection>
									</subSections>
								</ObjectPageSection>
								<ObjectPageSection
									titleUppercase="false"
									id="personalSection"
									title="Personal"
								>
									<subSections>
										<ObjectPageSubSection
											id="personalSectionSS1"
											title="Connect"
											titleUppercase="false"
										>
											<blocks>
												<personal:BlockPhoneNumber
													id="phone"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
												<personal:BlockSocial
													id="social"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
												<personal:BlockAdresses
													id="adresses"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
												<personal:BlockMailing
													id="mailing"
													columnLayout="1"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
											</blocks>
										</ObjectPageSubSection>
										<ObjectPageSubSection
											id="personalSectionSS2"
											title="Payment information"
											titleUppercase="false"
										>
											<blocks>
												<personal:PersonalBlockPart1
													id="part1"
													columnLayout="1"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
											</blocks>
											<moreBlocks>
												<personal:PersonalBlockPart2
													id="part2"
													columnLayout="1"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
											</moreBlocks>
										</ObjectPageSubSection>
									</subSections>
								</ObjectPageSection>
								<ObjectPageSection
									titleUppercase="false"
									id="employmentSection"
									title="Employment"
								>
									<subSections>
										<ObjectPageSubSection
											id="employmentSectionSS1"
											title="Job information"
											titleUppercase="false"
										>
											<blocks>
												<employment:BlockJobInfoPart1
													id="jobinfopart1"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
												<employment:BlockJobInfoPart2
													id="jobinfopart2"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
												<employment:BlockJobInfoPart3 id="jobinfopart3"/>
											</blocks>
										</ObjectPageSubSection>
										<ObjectPageSubSection
											id="employmentSectionSS2"
											title="Employee Details"
											titleUppercase="false"
										>
											<blocks>
												<employment:BlockEmpDetailPart1
													id="empdetailpart1"
													columnLayout="1"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
											</blocks>
											<moreBlocks>
												<employment:BlockEmpDetailPart2
													id="empdetailpart2"
													columnLayout="1"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
												<employment:BlockEmpDetailPart3
													id="empdetailpart3"
													columnLayout="1"
													class="sapUxAPObjectPageSubSectionAlignContent"
												/>
											</moreBlocks>
										</ObjectPageSubSection>
									</subSections>
								</ObjectPageSection>
							</sections>
						</ObjectPageLayout>
					</m:content>
				</m:Page>
			</m:pages>
		</m:App>
	</m:Shell>
</mvc:View>